{
  "name": "dset-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@firebase/auth": "^1.9.0",
    "@firebase/firestore": "^4.7.8",
    "@hookform/resolvers": "^3.10.0",
    "@react-spring/web": "^9.7.5",
    "canvas-confetti": "^1.9.3",
    "classnames": "^2.5.1",
    "cra-template": "1.2.0",
    "firebase": "^10.14.1",
    "papaparse": "^5.5.2",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-qr-code": "^2.0.15",
    "react-router-dom": "^6.29.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^4.2.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config webpack.config.dev.js --mode development",
    "dev:init": "ts-node --project tsconfig.webpack.json scripts/init-dev-env.ts",
    "build": "webpack --config webpack.config.ts",
    "start": "cross-env NODE_ENV=development TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack serve --config webpack.config.dev.ts",
    "emulators": "firebase emulators:start",
    "emulators:export": "firebase emulators:export ./emulator-data",
    "emulators:start": "firebase emulators:start --import=./emulator-data --export-on-exit",
    "dev:app": "webpack serve --config webpack.config.dev.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "analyze": "ANALYZE=true webpack --mode production",
    "check-size": "ts-node scripts/check-bundle-size.ts",
    "track-size": "ts-node scripts/analyze-bundle.ts",
    "predev": "node scripts/check-dev-deps.mjs",
    "seed:auth": "cross-env TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\",\"isolatedModules\":false}' ts-node ./scripts/seed-auth.ts",
    "dev:emulate": "npm-run-all -p emulators dev",
    "emulators:init": "cross-env TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\",\"isolatedModules\":false}' ts-node scripts/init-emulators.ts",
    "seed:users": "cross-env TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/seed-auth.ts",
    "populate:known-users": "cross-env TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node scripts/populate-known-users.ts",
    "create:users-template": "cp ./data/known-users-template.csv ./data/known-users.csv",
    "migrate:roles": "cross-env TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node scripts/migrate-roles.ts",
    "populate:users": "cross-env TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/populate-known-users.ts",
    "populate:users:dev": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/populate-known-users.ts",
    "upload:photos": "cross-env TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/upload-user-photos.ts",
    "upload:photos:dev": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/upload-user-photos.ts",
    "update:photo": "cross-env TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/update-user-photo.ts",
    "update:photo:dev": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/update-user-photo.ts",
    "update:photos": "cross-env TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/update-user-photos.ts",
    "update:photos:dev": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/update-user-photos.ts",
    "load:users": "cross-env TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/load-users.ts",
    "load:users:dev": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/load-users.ts",
    "emulators:setup": "firebase init emulators",
    "prestart": "cross-env NODE_ENV=development ts-node --project tsconfig.scripts.json -r dotenv/config scripts/setup-dev-env.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/plugin-transform-typescript": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@faker-js/faker": "^9.5.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/csv-parse": "^1.1.12",
    "@types/jest": "^29.5.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.10.0",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@types/webpack-dev-server": "^4.7.2",
    "@types/webpack-env": "^1.18.8",
    "@types/yup": "^0.29.14",
    "axe-core": "^4.10.2",
    "babel-loader": "^9.2.1",
    "compression-webpack-plugin": "^11.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.4.7",
    "dotenv-webpack": "^8.1.0",
    "firebase-tools": "^13.30.0",
    "html-webpack-plugin": "^5.6.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "msw": "^2.7.0",
    "npm-run-all": "^4.1.5",
    "process": "^0.11.10",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^4.9.5",
    "webpack": "^5.97.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
